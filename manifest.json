{
  "name": "mFus",
  "version": "0.2",
  "description": "A shortcut for mFus shortener.",
  "permissions": [
    "tabs"
  ],
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "64": "icon64.png",
    "128": "icon128.png",
    "512": "icon512.png"
  },
  "browser_action": {
    "default_title": "mFus",
    "default_icon": "icon64.png",
    "default_popup": "popup.html"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self'; object-src 'self'"
}
